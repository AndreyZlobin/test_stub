{
  "name": "stub-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "ui-stub-example",
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint-css": "stylelint ./src/**/*.css",
    "lint-scripts": "eslint ./ ./src/ ./config/ ./cypress --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-types": "tsc",
    "eslint": "eslint --cache --ext .ts . --config .eslintrc.js",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx  . --fix --config .eslintrc.cjs",
    "plop": "npx plop",
    "test": "arui-scripts test"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@cypress/code-coverage": "^3.11.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "commitlint": "^17.4.4",
    "cypress": "^12.17.3",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "plop": "3.1.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  }
}
