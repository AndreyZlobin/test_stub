    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "12.7.0",
    "eslint": "^8.44.0",
    "plop": "3.1.1",
    "prettier": "2.8.1",
    "eslint-plugin-react-hooks": "^4.6.0",
